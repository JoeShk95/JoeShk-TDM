public lsccd(playerid)  {

	new finish[200] = true;
	new str[256];

	if(mils[playerid] > 0) finish[playerid] = false;
	else if(!mils[playerid])
	{
	    if(sec[playerid] > 0)
		{
			 sec[playerid] --;
			 mils[playerid] = 9;
			 finish[playerid] = false;
			 }
		     else if(!sec[playerid])
			    {
				if(lscmin[playerid] > 0)
				{
				    lscmin[playerid]--;
			        sec[playerid] = 60;
			        mils[playerid] = 9;
			        finish[playerid] = false;
			        }
					else if(!lscmin[playerid]) finish[playerid] = true;
				}
			}
	if(!finish[playerid]) mils[playerid]--;
	format(str,sizeof(str)," %d ~n~ %d / %d",mils[playerid],lscmin[playerid],sec[playerid]);
	if(IsPlayerConnected(playerid)) GameTextForPlayer(playerid,str,1000,6); else finish[playerid] = true;
	if(finish[playerid] || (lscmin[playerid] < 1 && sec[playerid] < 1)) cencellsc(playerid),lscwinner();
	if(!finish[playerid]) SetTimerEx("lsccd",100,0,"i",playerid);
}

