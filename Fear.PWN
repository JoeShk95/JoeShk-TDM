forward RacerCheckPlayers();
public RacerCheckPlayers()
{
	new i;
	for (i=0; i<MAX_PLAYERS; i++)
	{
	    if (active_players[i])
		{
			if (!IsPlayerConnected(i))
			{
				active_players[i] = false;
				DestroyVehicle(vehicles[i]);
			}
			else
				if (!IsPlayerInVehicle(i,vehicles[i]))
				{
					active_players[i] = false;
					DisablePlayerRaceCheckpoint(i);
					DestroyVehicle(vehicles[i]);
					SendClientMessage(i,red,"!נפסלת מהפעילות מירוץ מכוניות מירוץ");
				}
		}
 	}

	if (GetActivePlayers() == 1)
	{
	    KillTimer(Racer_Timer);
		for (i=0; i<MAX_PLAYERS; i++)
		{
		    if (active_players[i])
		    {
		        if (Place < 4) WinnerRacer(i,Place);
                ResetRacer(true);
     			break;
		    }
		}
	}
	else
	{
		if (GetActivePlayers() == 0)
		{
		    KillTimer(Racer_Timer);
		    ResetRacer(false);
			SendClientMessageToAll(red,"!קרה מצב של תיקו בין שני שחקנים לכן לא נקבע מנצח");
		}
	}
}

ResetRacer(bool:mode)
{
	new i;
    RaceRacer = false;
    CanJoin = false;
    Place = 1;
    KillTimer(Racer_Timer);
	for (i=0; i<MAX_PLAYERS; i++)
	{
	    if (IsPlayerConnected(i))
	    {
	        if (active_players[i])
	        {
	            DisablePlayerRaceCheckpoint(i);
	            RemovePlayerFromVehicle(i);
	            DestroyVehicle(vehicles[i]);
	            active_players[i] = false;
	            if (mode) SpawnPlayer(i);
	        }
		}
	}
}


