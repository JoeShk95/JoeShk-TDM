		{
		    if (IsPlayerConnected(i) && active_players[i])
		    {
    			TogglePlayerControllable(i,true);
				PlayerPlaySound(i,1057, 0.0, 0.0, 0.0);
				SetPlayerRaceCheckpoint(i,3,CPB[0][0],CPB[0][1],CPB[0][2],CPB[1][0],CPB[1][1],CPB[1][2],10);
			}
		}
		RCB_Timer = SetTimer("RCBCheckPlayers",1000,true);
	}
}

forward RCBCheckPlayers();
public RCBCheckPlayers()
{
	new i;
	for (i=0; i<MAX_PLAYERS; i++)
	{
	    if (active_players[i])
		{
			if (!IsPlayerConnected(i))
			{
				active_players[i] = false;
				DestroyVehicle(vehicles[i]);
			}
			else
				if (!IsPlayerInVehicle(i,vehicles[i]))
				{
					active_players[i] = false;
					DisablePlayerRaceCheckpoint(i);
					DestroyVehicle(vehicles[i]);
					SendClientMessage(i,ORANGE," ! נפסלת מהפעילות מירוץ מטוסי צעצוע ");
				}
		}
 	}

	if (GetActivePlayers() == 1)
	{
	    KillTimer(RCB_Timer);
		for (i=0; i<MAX_PLAYERS; i++)
		{
		    if (active_players[i])
		    {
		        if (Place < 4) WinnerRCB(i,Place);
                ResetRCB(true);
     			break;
		    }
		}
	}
	else
	{
		if (GetActivePlayers() == 0)
		{
		    KillTimer(RCB_Timer);
		    ResetRCB(false);
			SendClientMessageToAll(red,"!אין מנצח הפעם בתחרות");
		}
	}
}

ResetRCB(bool:mode)
{
	new i;
    RaceRCB = false;
    CanJoin = false;
    Place = 1;
    KillTimer(RCB_Timer);
	for (i=0; i<MAX_PLAYERS; i++)
	{
	    if (IsPlayerConnected(i))
	    {
	        if (active_players[i])
	        {
	            DisablePlayerRaceCheckpoint(i);
	            RemovePlayerFromVehicle(i);
	            DestroyVehicle(vehicles[i]);
	            active_players[i] = false;
	            if (mode) SpawnPlayer(i);
	        }
		}
	}
}

