}

forward finishrace();
public finishrace()
{
new isin = 0;
for(new i=0;i<MAX_PLAYERS;i++) { if(isinrace2[i] == 1 && IsPlayerConnected(i))  { isin++; } }
if(isin == 0)
{
for(new i=0;i<MAX_PLAYERS;i++)
{
isinrace2[i] = 0;
isinrace[i] = false;
}
SendClientMessageToAll(COLOR_LIGHTGREEN,"!המירוץ הסתיים, מזל טוב לזוכים");
race = false;
soper = 0;
winner =0;
isin = 0;
}
return true;
}


