forward NFSCountDown(Count_NFS);
public NFSCountDown(Count_NFS)
{
	new i;
	if(Count_NFS > 0)
	{
		new string[25];
		format(string, sizeof(string),"%s%d",(Count_NFS > 30) ? ("~w~") : ("~r~"),Count_NFS);
		for (i=0; i<MAX_PLAYERS; i++) if (IsPlayerConnected(i)) PlayerPlaySound(i,1058, 0.0, 0.0, 0.0);
		GameTextForAll(string,1050,6);
		Count_NFS--;
		SetTimerEx("NFSCountDown",1000,false,"i",Count_NFS);
	}
	else
	{
	    CanJoin = false;
	    if (GetActivePlayers() > 0)
		{
		    CreateBarrels();
		    CreateBikesPutPlayers();
		    for (i=0; i<MAX_PLAYERS; i++)
				if (IsPlayerConnected(i) && active_players[i])
				{
					SetPlayerRaceCheckpoint(i,4,1350.1152,-2489.4216,21.5375,1282.0624,-2492.6616,21.5375,10);
					SendClientMessage(i,0x0B01AFF,".- - - - - [Races Of Death] - - - - - ");
					SendClientMessage(i,lblue,".תחרות המירוץ החלה, המטרה להגיע לסוף בלי להתפוצץ");
					SendClientMessage(i,0x0B01AFF,".- - - - - - - - - - - - - - ");
				}
			Barrels_Timer = SetTimer("MoveBarrels",1000,true);
			SetTimerEx("StartNFSActivity",2000,false,"i",10);
		}
		else
		{
		    ResetNFS(false);
		    SendClientMessageToAll(red,"!הפעילות בוטלה בגלל שפחות מ-2 שחקנים הצטרפו");
		}
	}
}

