forward CheckAcceptPlayer();
public CheckAcceptPlayer()
{
	Duel[time]++;
	if (!Duel[accept] && (Duel[time] == 30))
	{
	    TogglePlayerControllable(Duel[playerid1],true);
	    SendClientMessage(Duel[playerid1],red,"!אף אחד לא אישר את ההזמנה שלך");
	    SpawnPlayer(Duel[playerid1]);
	    LACY_GiveMoney(Duel[playerid1],Duel[bet_duel]);
	    ResetDuelArray();
        KillTimer(Duel_Timer);
	}

	if (Duel[time] >= 300)
	{
        TogglePlayerControllable(Duel[playerid1],true);
        TogglePlayerControllable(Duel[playerid2],true);
        SpawnPlayer(Duel[playerid1]);
        SpawnPlayer(Duel[playerid2]);
        LACY_GiveMoney(Duel[playerid1],Duel[bet_duel]);
        LACY_GiveMoney(Duel[playerid2],Duel[bet_duel]);
        SendClientMessage(Duel[playerid1],red,"!זמן הכולל של הקרב עבר את המקסימום");
        SendClientMessage(Duel[playerid2],red,"!הזמן הכולל של הקרב עבר את המקסימום");
        ResetDuelArray();
        KillTimer(Duel_Timer);
	}

	if (!IsPlayerConnected(Duel[playerid1]))
	{
        TogglePlayerControllable(Duel[playerid2],true);
        SendClientMessage(Duel[playerid1],red,"!ניצחת ניצחון טכני");
		SpawnPlayer(Duel[playerid2]);
		LACY_GiveMoney(Duel[playerid2],2*Duel[bet_duel]);
        ResetDuelArray();
        KillTimer(Duel_Timer);
	}

	if (!IsPlayerConnected(Duel[playerid2]))
	{
        TogglePlayerControllable(Duel[playerid1],true);
        SendClientMessage(Duel[playerid1],red,"!ניצחת ניצחון טכני");
        SpawnPlayer(Duel[playerid1]);
        LACY_GiveMoney(Duel[playerid1],2*Duel[bet_duel]);
        ResetDuelArray();
        KillTimer(Duel_Timer);
	}
}

