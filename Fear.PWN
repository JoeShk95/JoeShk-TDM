forward SPCountDown(Count_SP);
public SPCountDown(Count_SP)
{
	new i;
	if(Count_SP > 0)
	{
		new string[25];
		format(string, sizeof(string),"%s%d",(Count_SP > 30) ? ("~w~") : ("~r~"),Count_SP);
		for (i=0; i<MAX_PLAYERS; i++) if (IsPlayerConnected(i)) PlayerPlaySound(i,1058, 0.0, 0.0, 0.0);
		GameTextForAll(string,1050,6);
		Count_SP--;
		SetTimerEx("SPCountDown",1000,false,"i",Count_SP);
	}
	else
	{
	    CanJoin = false;
	    if (GetActivePlayers() > 1)
		{
		    new fighter = 1;
		    for (i=0; i<MAX_PLAYERS; i++)
		    {
				if (IsPlayerConnected(i) && active_players[i])
 				{
					SendClientMessage(i,lblue,"~~~ (SP - ברוכים הבאים לפעילות מטווח) ~~~");

					switch(teams[i])
					{
				 		case 1 :
			 			{
                            place_fighter[i] = fighter;
						 	SetPlayerFighterPos(i,fighter);
						 	SendClientMessage(i,ORANGE,"!המטרה שלך בתור יורה היא להרוג כמה שיותר אנשים");
						 	fighter++;
					 	}
				 		case 2 :
				 		{
						 	SetPlayerRunnerPos(i);
						 	SendClientMessage(i,ORANGE,"!המטרה שלך בתור בורח היא למות כמה שפחות פעמים");
						 	SetPlayerWorldBounds(i,816.2601,787.9502,1656.7692,1611.2563);
					 	}
					}
				}
			}
			SetTimerEx("StartSPActivity",2000,false,"i",10);
		}
		else
		{
		    ResetSP(false);
			SendClientMessageToAll(red,"!הפעילות בוטלה בגלל שפחות מ-2 שחקנים הצטרפו");
}
	}
}


