EndCTF()
{
SendClientMessageToAll(red,"!לא התחיל בגלל שאחת מהקבוצות ריקה Capture The Flag ה");

for(new i = 0; i < ToDel; i++) DestroyStreamObject(Del_Object_CTF[i]);

for (new i = 0;  i < MAX_PLAYERS; i++)
{
if (IsPlayerConnected(i))
{
if(CtfpInfo[i][RedTeam])
{
SpawnPlayer(i);
LACY_GiveMoney(i,5000);
SetPlayerVirtualWorld(i,0);
CtfpInfo[i][RedTeam] = 0;
CtfInfo[States] = 0;
RemovePlayerMapIcon(i,PRemove[i][0]);
RemovePlayerMapIcon(i,PRemove[i][1]);
CtfpInfo[i][HasFlag] = 0;
}}}


for (new i = 0;  i < MAX_PLAYERS; i++)
{
if (IsPlayerConnected(i))
{
if(CtfpInfo[i][GreenTeam])
{
SpawnPlayer(i);
SetPlayerVirtualWorld(i,0);
CtfpInfo[i][GreenTeam] = 0;
CtfInfo[States] = 0;
RemovePlayerMapIcon(i,PRemove[i][0]);
RemovePlayerMapIcon(i,PRemove[i][1]);
CtfpInfo[i][HasFlag] = 0;
}}}
return true;
}


