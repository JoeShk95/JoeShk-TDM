public MegaRotate()
{
if(!Timer_Rotate[1]) return false;
	new Float:y,Float:y2,Float:z,Float:dis;
	for(new i=0; i<19; i++)
	{
	    GetStreamObjectRot(Spokes1[i],y2,y2,z);
		SetStreamObjectRot(Spokes1[i],0,y2-=0.25,270);
		y=1526.9259; z=55.33241;
		GetXYInDirectionOfPosition(y2,y,z,14.09467);
		dis = floatsqroot(((1526.9259 - y) * (1526.9259 - y)) + ((z - 55.33241) * (z - 55.33241)));
		SetStreamObjectPos(Spokes1[i],1943.8535,y,z-dis);

	    GetStreamObjectRot(Spokes2[i],y2,y2,z);
		SetStreamObjectRot(Spokes2[i],0,y2-=0.25,270);
		y=1526.9259; z=55.33241;
		GetXYInDirectionOfPosition(y2,y,z,14.09467);
		dis = floatsqroot(((1526.9259 - y) * (1526.9259 - y)) + ((z - 55.33241) * (z - 55.33241)));
		SetStreamObjectPos(Spokes2[i],1940.8535,y,z-dis);

		y=1526.9259; z=55.33241;
