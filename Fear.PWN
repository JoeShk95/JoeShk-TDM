TogglePlayerControllable(playerid,true);
isinDrift[playerid] = false;
Drifton = false;
drift[playerid] =0;
LACY_GiveMoney(playerid,driftprice);
return 1;
}
if(IsPlayerConnected(playerid) && IsPlayerConnected(playerid2))
{
GameTextForPlayer(playerid,"~R~ DRIFT TIME !!!",3500,4);
GameTextForPlayer(playerid2,"~R~ DRIFT TIME !!!",3500,4);
drift[playerid] =0;
drift[playerid2] =0;
TogglePlayerControllable(playerid,true);
TogglePlayerControllable(playerid2,true);
switch(modeldrift)
{
	case 1:
